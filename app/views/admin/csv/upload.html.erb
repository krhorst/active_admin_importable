<%= form_for :dump, :url => {:action => target_action}, :html => {:multipart => true} do |f| %>
 <table>
   <tr>
     <td>
       <%= label_tag  "dump_file", "Select a CSV File" %>
       </td>
     <td>
       <%= f.file_field :file %>
     </td>
   </tr>
   <tr>
     <td>
       <%= submit_tag 'Submit' %>
     </td>
   </tr>
 </table>
<% end %>
